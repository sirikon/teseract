{
  "name": "teseract",
  "version": "2.8.0",
  "bin": {
    "teseract": "bin/teseract.js"
  },
  "scripts": {
    "example": "cd example && ts-node -P ../tsconfig.json ../src/main.ts",
    "example-empty": "mkdir -p example-empty && cd example-empty && ts-node -P ../tsconfig.json ../src/main.ts",
    "build": "tsc --declaration --project .",
    "prepare": "npm run -s build"
  },
  "files": [
    "lib/**/*",
    "bin/*"
  ],
  "devDependencies": {
    "@types/eslint": "^7.28.0",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^16.6.0",
    "@types/sass": "^1.16.1"
  },
  "dependencies": {
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "chai": "^4.3.4",
    "chokidar": "^3.5.2",
    "esbuild": "^0.12.19",
    "esbuild-plugin-sass": "^0.6.0",
    "eslint": "^7.32.0",
    "mime-types": "^2.1.32",
    "mocha": "^9.0.3",
    "superstruct": "^0.15.2",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5"
  }
}
