{
  "name": "teseract",
  "version": "2.12.0",
  "bin": {
    "teseract": "bin/teseract.js"
  },
  "scripts": {
    "example": "cd example && ts-node -P ../tsconfig.json ../src/main.ts",
    "example-empty": "mkdir -p example-empty && cd example-empty && ts-node -P ../tsconfig.json ../src/main.ts",
    "build": "tsc --declaration --project .",
    "prepare": "npm run -s build"
  },
  "files": [
    "lib/**/*",
    "bin/*"
  ],
  "devDependencies": {
    "@types/eslint": "^8.4.1",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^16.11.26",
    "@types/sass": "^1.43.1",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "chai": "^4.3.6",
    "chokidar": "^3.5.3",
    "esbuild": "^0.14.30",
    "esbuild-plugin-sass": "^1.0.1",
    "eslint": "^8.12.0",
    "mime-types": "^2.1.35",
    "mocha": "^9.2.2",
    "superstruct": "^0.15.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
