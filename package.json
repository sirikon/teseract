{
  "name": "teseract",
  "version": "2.9.0",
  "bin": {
    "teseract": "bin/teseract.js"
  },
  "scripts": {
    "example": "cd example && ts-node -P ../tsconfig.json ../src/main.ts",
    "example-empty": "mkdir -p example-empty && cd example-empty && ts-node -P ../tsconfig.json ../src/main.ts",
    "build": "tsc --declaration --project .",
    "prepare": "npm run -s build"
  },
  "files": [
    "lib/**/*",
    "bin/*"
  ],
  "devDependencies": {
    "@types/eslint": "^8.2.2",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^16.11.19",
    "@types/sass": "^1.43.1"
  },
  "dependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "chai": "^4.3.4",
    "chokidar": "^3.5.2",
    "esbuild": "^0.14.11",
    "esbuild-plugin-sass": "^1.0.1",
    "eslint": "^8.7.0",
    "mime-types": "^2.1.34",
    "mocha": "^9.1.4",
    "superstruct": "^0.15.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
